int findUnique(int *arr, int size)
{
    int ans=0;
    for(int i=0;i<size;i++){
        ans= ans^arr[i];
    }
    return ans;
}



#include<bits/stdc++.h>
using namespace std;

int main()
{

int n;
cin>>n;
int arr[n];
int ans=0;
for(int i=0;i<n;i++){
    cin>>arr[i];
}
for(int i=0;i<n;i++){
    ans=ans^arr[i];
}
    
cout<<ans;
      
     
}

// output 
// 5
// 12 11 10 12 11
// 10
